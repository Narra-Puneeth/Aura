{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "VITE_FITBIT_ACCESS_KEY": "@fitbit_access_key",
    "VITE_GOOGLE_AI_API_KEY": "@google_ai_api_key",
    "TAVILY_API_KEY": "@tavily_api_key"
  }
}
